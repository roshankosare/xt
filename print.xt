var a = 0;
var MSIZE = 0;
var MMEM = 0;
var STR_LENGTH = 0;
var STR_POINTER = 0;
function memalloc(size){
   MSIZE = size;
   asm([
      "mov eax , [MSIZE]",
      "mov ecx , eax",
      "call malloc",
      "mov bl , [REF_TYPE]",
      "movzx bx , bl",
      "mov [MMEM] , bx",
      "mov [MMEM + 1] , eax",
   ]);
  return MMEM;
}


function print(x){

   var length = 0;
   var temp = x;
   var maxBuffer = 1024;
   var buffer = memalloc(maxBuffer);
   
   var tempBuffer = buffer;
   var ch = 0;
   ch = @temp;   
   while(ch != 0){
      ch = @temp;
      ch = ch & 0xFF;
       asm(["pop eax" , "call print_eax"]);
      if(ch == 92){ 
         // 92 is ASCII for  '/' character
         temp++;
         ch = @temp;
