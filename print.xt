         if(ch == 110){
            // 110 is ASCII for 'n'
            ch = 10;  // 10 is ASCII for new line
         }
         if(ch == 116){
            // 116 is ASCII for 't'
            ch = 9; // 9 is ASSCII for tab
         }
      }

      //if(ch == )
      @tempBuffer = ch;
      temp++;
      tempBuffer++;
      length++;
   }

  
   
   STR_LENGTH = length;
   STR_POINTER = buffer;
   
  
   asm([    "mov ecx , [STR_POINTER]                           ; load the address of the string",
            "mov edx , [STR_LENGTH]                          ; load the length of the string",                   
            "mov eax, 4                        ; syscall number for sys_write",
            "mov ebx, 1                        ; file descriptor 1 (stdout)",             
            "int 0x80                          ; make syscall to write",
         ]);
      
 }

 
